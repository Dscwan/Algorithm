class Solution {
    public int solution(int storey) {
        int answer = 0;
        int n = 0;
        while(true){
            if(storey<=0) break;
            n = storey%10;
            if(n>5 || (n==5&&(storey/10)%10>=5)){
                answer += (10-n);
                storey /= 10;
                storey ++;
            }
            else {
                answer += n;
                storey /= 10;
            }
        }
        return answer;
    }
}
